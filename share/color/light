# builtin highlight colors
hi default
hi commandline
hi errormsg bold red
hi infomsg bold blue
hi selection keep gray keep
hi activetab bold
hi inactivetab black gray
hi tabbar black gray
hi statusline black gray
hi wserror default yellow
hi nontext blue keep

# common to many languages
hi keyword bold
hi type bold
hi label bold
hi notice bold red
hi numeric magenta
hi constant magenta
hi char magenta
hi string magenta
hi special bold red
hi comment bold blue
hi preproc blue
hi error black yellow

# diff
hi diff.header bold
hi diff.chunk black gray
hi diff.added blue
hi diff.removed bold red
hi diff.changed magenta
hi diff.context keep keep

# editor
hi editor.variable blue

# gitcommit
hi gitcommit.added bold
hi gitcommit.modified bold red
hi gitcommit.untracked bold red

# sh
hi sh.variable blue
hi sh.builtin bold
hi sh.operator bold

# go
hi go.builtin blue

# smarty
hi smarty blue
